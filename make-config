#!/bin/bash
. ./config

cat << EOF > config.R
# Autogenerated, do not edit
suffix<-"$suffix"
monthly<-$monthly
report_org<-$report_org
report_dl<-$report_dl
days<-$days
hours<-$hours
production_nodes<-$production_nodes
production_cores<-$production_cores
EOF

cat << EOF > config.py
# Autogenerated, do not edit
suffix="$suffix"
EOF
