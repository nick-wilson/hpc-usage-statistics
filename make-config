#!/bin/bash
. ./config

cat << EOF > config.R
# Autogenerated, do not edit
suffix<-"$suffix"
report_org<-$report_org
report_dl<-$report_dl
days<-$days
hours<-$hours
filter_dependent_jobs<-$filter_dependent_jobs
EOF

if [ x"$type" == x"monthly" ] ; then
 wslabel="${ey}-${em}"
else
 wslabel="${ey}-${em}-${ed}"
fi

cat << EOF > config.py
# Autogenerated, do not edit
suffix="$suffix"
wslabel="$wslabel"
EOF

./make-pull
