#!/bin/bash

. ./config

rawfile="pbs-report.raw.$suffix.csv"

if [ x$HOSTNAME$USER != xwlm01root ] ; then
 echo ssh to root@wlm01 to run pbs-report
 ssh root@wlm01 pbs-report --verbose --csv "|" --begin $sy$sm$sd --end $ey$em$ed --time full > "$rawfile"
else
 echo running pbs-report
 pbs-report --verbose --csv "|" --begin $sy$sm$sd --end $ey$em$ed --time full > "$rawfile"
fi

